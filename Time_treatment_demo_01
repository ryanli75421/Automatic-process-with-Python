import time
print(time.time()) #從1970/01/01開始到現在的秒數總和

#利用time計算程式執行時間
import time

def calculation():
    item = 1
    for i in range(1, 100000):
        item = item + 1
    return item

starttime = time.time()  #開始計時
treatment = calculation()
endtime = time.time()  #計時停止
print('結果顯示 %s 位元長.' % (len(str(treatment))))
print('花費 %s 秒完成計算.' % (endtime - starttime))

import time

for i in range(3):
    print('Tick')
    time.sleep(1)  #延遲秒數
    print('Tick')
    time.sleep(1)
    print('Tick')
    time.sleep(1)
    
    
import time
this_moment = time.time()
print(round(this_moment, 2)) #四捨五入到小數點後第二位
