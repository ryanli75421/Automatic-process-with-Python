
def factorial(n):
  if(n==0):
    return 0
  if(n==1):
    return 1
  else:
    return n*factorial(n-1)

print(factorial(1))
print(factorial(5))
print(factorial(10))

def fibonaci(n):
  if(n==0):
    return 0
  if(n==1):
    return 1
  else:
    return fibonaci(n-1)+fibonaci(n-2)
print(fibonaci(1))
print(fibonaci(2))
print(fibonaci(5))
print(fibonaci(10))
