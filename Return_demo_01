# Make a return

import random

def guessnation(number):
    
    if number == 1:
        return 'United States'
    elif number == 2:
        return 'Japan'
    elif number == 3:
        return 'China'
    elif number == 4:
        return 'United Kingdom'
    elif number == 5:
        return 'Idian'
#return特定值回變數

print(guessnation(random.randint(1,5)))
